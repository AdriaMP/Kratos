            "Parameters"            : {
                "output_variables" : ["VELOCITY_X","VELOCITY_Y","PRESSURE"],
                "output_file_name" : "multiple_overlapping_patches_fractionalstep_results.json",
                "model_part_name"  : "GENERIC_background",
                "time_frequency"   : 0.1
            }
        }
        ]
   },
