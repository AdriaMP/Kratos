         "input_type":"mdpa",
         "input_filename":"../mdpa_for_tests/multiple_overlapping_patches"
      },
      "echo_level":2,
      "compute_reactions":true,
      "dynamic_tau":1.0,
      "oss_switch":0,
      "volume_model_part_name":"Parts_all",
      "skin_parts":[
         "AutomaticInlet2D_inlet",
         "Outlet2D_outlet",
         "NoSlip2D_walls",
         "NoSlip2D_structure_1_1",
         "NoSlip2D_structure_2_1"
      ],
      "no_skin_parts":[
         "Drag_structure_1_1",
         "Drag_structure_2_1"
      ],
      "time_stepping":{
         "automatic_time_step":false,
         "time_step":0.01
      }
   },
   "processes":{
      "initial_conditions_process_list":[
         {
            "python_module":"assign_vector_variable_process",
            "kratos_module":"KratosMultiphysics",
            "Parameters":{
               "model_part_name":"FluidModelPart.VELOCITY_all",
               "variable_name":"VELOCITY",
               "constrained":[
                  false,
                  false,
                  false
               ],
               "value":[
                  0.1,
                  0.0,
                  0.0
               ]
            }
         },
         {
            "python_module":"assign_scalar_variable_process",
            "kratos_module":"KratosMultiphysics",
            "Parameters":{
               "model_part_name":"FluidModelPart.PRESSURE_all",
               "variable_name":"PRESSURE",
               "constrained":false,
               "value":0.0
            }
         }
      ],
      "boundary_conditions_process_list":[
         {
            "python_module":"apply_inlet_process",
            "kratos_module":"KratosMultiphysics.FluidDynamicsApplication",
            "Parameters":{
               "model_part_name":"FluidModelPart.AutomaticInlet2D_inlet",
               "variable_name":"VELOCITY",
               "modulus":"0.1",
               "direction":"automatic_inwards_normal",
               "interval":[
                  0.0,
                  "End"
               ]
            }
         },
         {
            "python_module":"apply_outlet_process",
            "kratos_module":"KratosMultiphysics.FluidDynamicsApplication",
            "Parameters":{
               "model_part_name":"FluidModelPart.Outlet2D_outlet",
               "variable_name":"PRESSURE",
               "constrained":true,
               "value":0.0,
               "hydrostatic_outlet":false,
               "h_top":0.0
            }
         },
         {
            "python_module":"apply_noslip_process",
            "kratos_module":"KratosMultiphysics.FluidDynamicsApplication",
            "Parameters":{
               "model_part_name":"FluidModelPart.NoSlip2D_walls"
            }
         },
         {
            "python_module":"apply_noslip_process",
            "kratos_module":"KratosMultiphysics.FluidDynamicsApplication",
            "Parameters":{
               "model_part_name":"FluidModelPart.NoSlip2D_structure_1_1"
            }
         },
         {
            "python_module":"apply_noslip_process",
            "kratos_module":"KratosMultiphysics.FluidDynamicsApplication",
            "Parameters":{
               "model_part_name":"FluidModelPart.NoSlip2D_structure_2_1"
            }
         }
      ],
      "gravity":[
         {
            "python_module":"assign_vector_by_direction_process",
            "kratos_module":"KratosMultiphysics",
            "process_name":"AssignVectorByDirectionProcess",
            "Parameters":{
               "model_part_name":"FluidModelPart.Parts_all",
               "variable_name":"BODY_FORCE",
               "modulus":0.0,
               "constrained":false,
               "direction":[
                  0.0,
                  -1.0,
                  0.0
               ]
            }
         }
      ],
      "auxiliar_process_list":[

      ],
