{
   "problem_data":{
      "problem_name":"fractional_step_multi_patch",
      "parallel_type":"OpenMP",
      "echo_level"    : 0,
      "start_time":0.0,
      "end_time":0.11
   },
   "output_processes":{
   },
   "solver_settings":{
      "model_part_name":"FluidModelPart",
      "domain_size":2,
